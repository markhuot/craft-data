{
    "name": "markhuot/craft-data",
    "type": "craft-plugin",
    "require": {
        "craftcms/cms": "^4.2",
        "hashids/hashids": "^4.1",
        "markhuot/data": "^1.0"
    },
    "require-dev": {
        "markhuot/craft-pest": "@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Huot",
            "email": "mark@markhuot.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true,
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "markhuot\\craftdata\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Data",
        "handle": "data",
        "class": "markhuot\\craftdata\\Data"
    }
}
